{
  "name": "svelte-vite-ssr",
  "version": "0.0.0",
  "scripts": {
    "dev:csr": "VITE_SVELTE_HYDRATABLE=0 vite",
    "dev:ssr": "VITE_SVELTE_HYDRATABLE=1 DEV=1 ts-node --compiler-options '{ \"module\": \"CommonJS\" }' src/server.ts",
    "build:csr": "VITE_SVELTE_HYDRATABLE=0 vite --emptyOutDir build",
    "build:ssr": "VITE_SVELTE_HYDRATABLE=1 vite --emptyOutDir build && vite build --ssr entry-server.ts",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-enough": "^0.7.0",
    "eslint-plugin-ext": "^0.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "postcss-load-config": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.40.0",
    "rollup-plugin-import-meta-url-to-module": "^0.2.8",
    "svelte-preprocess": "^4.6.9",
    "svelte-preprocess-css-hash": "^0.1.3",
    "svelte-preprocess-html-asset": "^0.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "vite": "^2.0.5",
    "vite-plugin-svelte-ssr-hot": "^8.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "svelte": "^3.35.0",
    "svelte-pilot": "^0.1.0-4",
    "teleman": "^0.6.1"
  }
}
